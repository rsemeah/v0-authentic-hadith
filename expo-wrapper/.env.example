# RevenueCat API Keys
# Get these from https://app.revenuecat.com → Project Settings → API Keys
REVENUECAT_API_KEY_APPLE=appl_xxxxxxxxxxxx
REVENUECAT_API_KEY_GOOGLE=goog_xxxxxxxxxxxx

# EAS Project ID
# Run `eas init` to get this, or find it at https://expo.dev
EAS_PROJECT_ID=

# Apple App Store Connect (for submission)
APPLE_ID=your@email.com
ASC_APP_ID=123456789
APPLE_TEAM_ID=XXXXXXXXXX

# Google Play (for submission)
# Path to your Google Play service account JSON key
GOOGLE_SERVICE_ACCOUNT_KEY_PATH=./google-play-key.json
